    <div class="container" style="margin-top: 100px; margin-right: 100px;">
        <form id="frmInfromacionTiquetesDescuento" class="form-horizontal">
            <fieldset id="fieldset">

                <div class="form-group row">
                    <label class="col-md-1 control-label" for="codigo">Código: </label>
                    <div class="col-md-5">
                        <input id="codigo" name="codigo" type="number" placeholder="Código"
                            class="form-control input-md">
                    </div>
                </div>

                <div class="form-group row">
                    <label class="col-md-1 control-label" for="descripcion">Descripción: </label>
                    <div class="col-md-5">
                        <input id="descripcion" name="descripcion" type="text" placeholder="Descripción"
                            class="form-control input-md">
                    </div>
                </div>

                <div class="form-group row">
                    <label class="col-md-1 control-label" for="disponibles">Disponibles: </label>
                    <div class="col-md-5">
                        <input id="disponibles" name="disponibles" type="number" placeholder="Disponibles"
                            class="form-control input-md">
                    </div>
                </div>

                <div class="form-group row">
                    <label class="col-md-1 control-label" for="descuento">Descuento en Porcentaje: </label>
                    <div class="col-md-5">
                        <input id="descuentoPorcentaje" name="descuentoPorcentaje" type="number" placeholder="Descuento Porcentaje"
                            class="form-control input-md">
                    </div>
                </div>

                <div class="form-group row">
                    <label class="col-md-1 control-label" for="descuento">Descuento en Monto: </label>
                    <div class="col-md-5">
                        <input id="descuentoMonto" name="descuentoMonto" type="number" placeholder="Descuento Monto"
                            class="form-control input-md">
                    </div>
                </div>

                <div class="form-group row">
                    <label class="col-md-2 control-label" for="btnRedondo"></label>
                    <div class="col-md-8">
                        <button id="btnRedondoCrearConsecutivo" name="btnGuardar" type="button" class="btn btn-primary">
                            Crear</button>
                        <button id="btnRedondoCrearConsecutivo" name="btnEliminar" type="button" class="btn btn-primary">
                            Eliminar</button>
                        <button id="btnRedondoCrearConsecutivo" name="btnModificar" type="button" class="btn btn-primary">
                            Modificar</button>
                    </div>
                </div>

